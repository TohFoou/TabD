{
  "manifest_version": 2,

  "name": "TAB'D",
  "description": "This extension is a tab manager and task scheduler, put together!",
  "version": "0.0",

  "background": {
      "persistent" : false
  },
  
  "browser_action": {
    "default_popup": "popup.html"
  },
  
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
